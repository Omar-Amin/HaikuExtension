{
  "manifest_version": 2,
  "name": "HaikuExtension",
  "description": "This extension makes a playlist of top videos from r/youtubehaiku and puts in a sync-vid playlist.",
  "version": "1.0.1",
  "permissions": ["https://*/*","http://*/*","tabs"],
  "background": {
    "scripts": ["popup.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
